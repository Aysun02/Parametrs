using System;

namespace ConsoleApp35
{
    class Program
    {
        static void Main(string[] args)
        { 
            Square(5, 5);
        }
        static void  Square(int width, int height)
        {

            for (int i = 0; i <= width; i++)

            {
                for (int j = 0; j < i; j++)

                {
                    Console.Write("*");
                }
                Console.WriteLine();

            }
        }

    }
}

